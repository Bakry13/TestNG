<suite name="AutomationPractice" parallel="tests" thread-count = "2">
 	<test name="validLogin">
		<classes>
			<class name="features.Parallelization">
				<methods>
					<include name="validLogin" />
				</methods>
			</class>
		</classes>
	</test>
 	<test name="invalidLogin">
		<classes>
			<class name="features.Parallelization">
				<methods>
					<include name="invalidLogin" />
				</methods>
			</class>
		</classes>
	</test>
</suite>